	<TextView xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/EventListItemType" android:layout_width="wrap_content"
		android:layout_height="wrap_content" android:padding="10dp"
		android:textSize="10sp">
	</TextView>
	<TextView xmlns:android="http://schemas.android.com/apk/res/android"
		android:id="@+id/EventListItemText" android:layout_height="wrap_content" android:padding="2dp"
		android:textSize="10sp" android:layout_width="wrap_content">
