<?xml version="1.0" encoding="utf-8"?>
<LinearLayout 
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res/com.astromaximum.android.view"
    android:orientation="vertical"
    android:layout_width="fill_parent"
    android:layout_height="fill_parent"
    >
<com.astromaximum.android.view.VocEventView
    android:layout_width="fill_parent" 
    android:layout_height="20dip" 
    android:id="@+id/event0"
	/>
	<LinearLayout 
		xmlns:android="http://schemas.android.com/apk/res/android"
		xmlns:app="http://schemas.android.com/apk/res/com.astromaximum.android.view"
	    android:orientation="horizontal"
	    android:layout_width="fill_parent"
	    android:layout_height="fill_parent"
	    >
	<com.astromaximum.android.view.EventView
	    android:layout_width="22dip" 
	    android:layout_height="fill_parent" 
	    android:id="@+id/event1"
		/>
	<com.astromaximum.android.view.EventView
	    android:layout_width="22dip" 
	    android:layout_height="fill_parent" 
	    android:id="@+id/event2"
		/>
	<com.astromaximum.android.view.EventView
	    android:layout_width="fill_parent" 
	    android:layout_height="fill_parent" 
	    android:id="@+id/event3"
		/>
	</LinearLayout>
</LinearLayout>
